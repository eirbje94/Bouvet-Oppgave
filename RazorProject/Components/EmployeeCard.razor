
<div class="card m-2">
    <div class="card-body">
        <div class="card-text text-center">
            <h5>@Name</h5>
            <h6><b>ID</b>: @Id</h6>
        </div>
    </div>
</div>


@code {

    [Parameter]
    public string Name { get; set; }

    [Parameter]
    public int Id { get; set; }
}
